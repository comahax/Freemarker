.alertbtn,.conf{display:block;text-decoration:none}#popup_panel a,.alertbtn,.conf{text-decoration:none}#popup_container{width:90%;height:auto;background:#fff;-moz-border-radius:5px;-webkit-border-radius:5px;border-radius:5px;position:fixed;top:50%}#popup_title,.alertbtn,.conf{height:44px;color:#3fabec;line-height:44px}#popup_title{-webkit-box-sizing:border-box;font-size:20px;margin:0;padding:0 0 0 20px;text-align:center;font-weight:700}#popup_content{padding:0;margin:0}#popup_message{font-size:14px;padding:10px 20px 20px;text-align:left}#popup_panel{border-top:1px solid #3fabec}#popup_panel a{color:#3fabec}.alertbtn{width:100%;text-align:center}.conf{width:50%;float:left;box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box}.btncl{border-left:1px solid #3fabec}