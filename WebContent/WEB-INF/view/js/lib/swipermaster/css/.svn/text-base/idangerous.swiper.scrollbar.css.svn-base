.swiper-scrollbar {
	border-radius: 10px;
	position: relative;
	-ms-touch-action: none;
	background: rgba(0,0,0,0.1)
}
.swiper-scrollbar-drag {
	height: 100%;
	width: 100%;
	position: relative;
	background: rgba(204,89,34,0.85);
	border-radius: 10px;
}
.swiper-scrollbar-cursor-drag {
	cursor: move;
}


